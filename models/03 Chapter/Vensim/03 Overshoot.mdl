{UTF-8}
Capital Funds=
	MAX(0,Profit*Fraction Profits Reinvested)
	~	Euro
	~		|

Investment=
	MIN(Desired Investment,Maximum Investment)
	~	Well/Year
	~		|

Capital= INTEG (
	Investment-Depreciation,
		5)
	~	Well
	~	Well
	|

Capital Costs=
	Capital*0.1
	~	Euro
	~		|

Depreciation Rate=
	0.05
	~	Year
	~		|

Cost Per Investment=
	2
	~	Euro/Well
	~		|

Depreciation=
	Capital*Depreciation Rate
	~	Well/Year
	~		|

Desired Growth Fraction=
	0.07
	~	Dmnl
	~		|

Desired Investment=
	Capital*Desired Growth Fraction
	~	Well
	~		|

Extraction=
	Extraction Efficiency Per Unit Capital*Capital
	~	Oil/Year
	~		|

Extraction Efficiency Per Unit Capital= WITH LOOKUP (
	Resource,
		([(0,0)-(1000,1)],(0,0),(100,0.25),(200,0.45),(300,0.63),(400,0.75),(500,0.85),(600\
		,0.92),(700,0.96),(800,0.98),(900,0.99),(1000,1) ))
	~	(Oil/Well)/Year
	~		|

Fraction Profits Reinvested=
	0.12
	~	Dmnl
	~		|

Maximum Investment=
	Capital Funds/Cost Per Investment
	~	Well
	~		|

Profit=
	Total Revenue-Capital Costs
	~	Euro
	~		|

Resource= INTEG (
	-Extraction,
		1000)
	~	Oil
	~		|

Revenue Per Unit Extracted=
	3
	~	(Euro/Oil)/Year
	~		|

Total Revenue=
	Revenue Per Unit Extracted*Extraction
	~	Euro
	~		|

